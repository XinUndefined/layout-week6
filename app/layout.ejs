              <a
                href="#collapseExample"
                class="d-flex justify-content-between text-dark font-weight-600 text-decoration-none product"
                data-toggle="collapse"
                data-target="#collapseExample"
                >Product
                <span class="material-icons down">
                  expand_less
                </span>
                <span class="material-icons up d-none">
                  expand_more
                </span>
              </a>
            </li>
            <li class="collapse" id="collapseExample">
              <ul class="list-none pl-0 text-primary font-weight-600">
                <li class="mb-3">
                  <a href="product.html" class="text-decoration-none"
